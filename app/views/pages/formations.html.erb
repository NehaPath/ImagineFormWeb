<!doctype html>
<html lang="en">
<head>

<title>Formation Creator</title>

<body style= "background-color: #81CFE0">

<style>
#draggableElement{
	width: 50px; 
	height: 50px; 
	background:black; 
	border-radius: 50%;
}

#draggableElement.ui-draggable-dragging { background: green; }
}

#draggableElement2{
	width: 50px; 
	height: 50px; 
	background:black; 
	border-radius: 50%;
}

#draggableElement3{
	width: 50px; 
	height: 50px; 
	background:black; 
	border-radius: 50%
}

#background {
	width: 600px; 
	height: 500px; 
	background:white; 
	display: block;
	margin: 0 auto;
}
</style>

<script type="text/javascript" src="http://ajax.googleapis.com/ajax/libs/jquery/1.5.0/jquery.min.js"></script>
<script type="text/javascript" src="http://ajax.googleapis.com/ajax/libs/jqueryui/1.8.9/jquery-ui.min.js"></script>
<script type="text/javascript">

$( init );

function init() {

  $('.draggable').draggable(
  {
  	containment: '#background',
  	stop: coordinates
  });
}
    
function coordinates( event, ui ) {
    {
        var x = ui.position.left;
        var y = ui.position.top;
        console.log("drag stopped, current pos = "+ui.position.left+" "+ui.position.top+"", event, ui);
        alert( "Drag stopped!\n\nCoordinates: (" + x + ", " + y + ")\n");
    }
}

</script>

</head>
<body>

<div id="content" style="height: 400px;">
  <div id="background"> </div>
	</div>

<div id="content" style="height: 400px;">
  <div id="draggableElement" class='draggable'> </div>

  <div id="draggableElement2" class='draggable'> </div>

  <div id="draggableElement3" class='draggable'> </div>
	</div>


 
</body>
</html>